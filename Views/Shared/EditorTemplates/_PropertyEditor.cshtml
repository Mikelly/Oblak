@using Oblak.Models.Api
@model PropertyDto
@(Html.Kendo().DropDownListFor(m => m.MunicipalityId)
    .DataTextField("Name")
    .DataValueField("ExternalId")
    .DataSource(source =>
    {
        source.Read(read =>
        {
            read.Action("GetMunicipalityList", "Property");
        });
    })
)